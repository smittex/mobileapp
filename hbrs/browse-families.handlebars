<div class="pane product-list">
    <div class="contents-wrap">
        {{#each categories}}
            <h1>{{name}}</h1>
            {{#each families}}
               <a class="family list product-button" data-family="{{name}}">{{name}}
                	<div class="color match">
                	    <img src="img/right_arrow_black.png"/>
                	</div>
                </a>
                {{/each}}
            <br />
        {{/each}}
    </div>
</div>
